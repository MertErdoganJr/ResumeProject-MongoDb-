@model Education
@{
    ViewData["Title"] = "CreateEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Add New Education</h1>
<form method="post">
    <label>Education Name</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Education Description</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    <label>Education Years</label>
    <input type="text" asp-for="Year" class="form-control" />
    <br />
    <button class="btn btn-primary">Add New Education</button>
</form>

