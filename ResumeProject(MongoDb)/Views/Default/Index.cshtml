                    <a class="btn btn-primary py-3 px-5 mt-3" href="">Read More</a>
                </div>
                <div class="col-lg-6 wow fadeInUp" data-wow-delay="0.5s">
                    <div class="row g-3 mb-4">
                        <div class="col-sm-6">
                            <img class="img-fluid rounded" src="~/proman-1.0.0/img/about-1.jpg" alt="">
                        </div>
                        <div class="col-sm-6">
                            <img class="img-fluid rounded" src="~/proman-1.0.0/img/about-2.jpg" alt="">
                        </div>
                    </div>
                    <div class="d-flex align-items-center mb-3">
                        <h5 class="border-end pe-3 me-3 mb-0">Happy Clients</h5>
                        <h2 class="text-primary fw-bold mb-0" data-toggle="counter-up">1234</h2>
                    </div>
                    <p class="mb-4">Stet no et lorem dolor et diam, amet duo ut dolore vero eos. No stet est diam amet diam ipsum clita labore dolor duo clita.</p>
                    <div class="d-flex align-items-center mb-3">
                        <h5 class="border-end pe-3 me-3 mb-0">Projects Completed</h5>
                        <h2 class="text-primary fw-bold mb-0" data-toggle="counter-up">1234</h2>
                    </div>
                    <p class="mb-0">Stet no et lorem dolor et diam, amet duo ut dolore vero eos. No stet est diam amet diam ipsum clita labore dolor duo clita.</p>
                </div>
            </div>
        </div>
    </div>
    <!-- About End -->
    <!-- Expertise Start -->
    @await Component.InvokeAsync("_SkillsPartial")
    <!-- Expertise End -->
    <!-- Service Start -->
    @await Component.InvokeAsync("_ServicePartial")
    <!-- Service End -->
    <!-- Projects Start -->
    @await Component.InvokeAsync("_ProjectPartial")
    <!-- Projects End -->
    <!-- Testimonial Start -->
    @await Component.InvokeAsync("_TestimonialPatial")
    <!-- Testimonial End -->
    <!-- Contact Start -->

    @* @await Component.InvokeAsync("_ContactPartial") *@

    @await Html.PartialAsync("~/Views/Default/SendMessage.cshtml")
    <!-- Contact End -->
    <!-- Map Start -->
    @await Component.InvokeAsync("_MapPartial")
    <!-- Map End -->
    <!-- Copyright Start -->
    <div class="container-fluid bg-dark text-white py-4">
        <div class="container">
            <div class="row">
                <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                    &copy; Resume Porject | All Right Reserved | @DateTime.Now.Year
                </div>
            </div>
        </div>
    </div>
    <!-- Copyright End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    @await Component.InvokeAsync("_ScriptPartial")
    
    <!-- Template Javascript -->
    <script src="~/proman-1.0.0/js/main.js"></script>
</body>

</html>